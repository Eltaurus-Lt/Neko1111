{
  "manifest_version": 3,
  "name": "Neko1111",
  "version": "1.5",
  "description": "meow",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "downloads",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "content_security_policy": {},
  "web_accessible_resources": [{
    "resources": ["nekos/*"],
    "matches": ["<all_urls>"]
    }],
   "content_scripts": [
     {
       "matches": ["<all_urls>"],
       "css": ["nekostyle.css"]
     }
   ]
}
